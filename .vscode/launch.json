    "version": "0.2.0",
    "configurations": [
        {
            "name": "Azure Data Studio",
            "type": "sqlopsExtensionHost",
            "request": "launch",
            "runtimeExecutable": "azuredatastudio",
            "args": ["--extensionDevelopmentPath=${workspaceRoot}/packages/azdata-sandance" ],
            "stopOnEntry": false,
            "sourceMaps": true,
            "outFiles": [ "${workspaceRoot}/packages/azdata-sandance/out/**/*.js" ],
            "preLaunchTask": "",
            "timeout": 60000
        },
    ]
}